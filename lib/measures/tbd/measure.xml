  <class_name>TBDMeasure</class_name>
  <display_name>Thermal Bridging and Derating - TBD</display_name>
  <description>Thermally derates opaque constructions from major thermal bridges</description>
  <modeler_description>(see github.com/rd2/tbd)</modeler_description>
  <arguments>
    <argument>
      <name>load_tbd_json</name>
      <display_name>Load tbd.json</display_name>
      <description>Loads existing tbd.json from model files directory, overrides other arguments if true.</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>false</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>option</name>
      <display_name>Default thermal bridge option to use if not reading tbd.json</display_name>
      <description>e.g. poor, regular, efficient, code</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>poor (BC Hydro)</default_value>
      <choices>
        <choice>
          <value>poor (BC Hydro)</value>
          <display_name>poor (BC Hydro)</display_name>
        </choice>
        <choice>
          <value>regular (BC Hydro)</value>
          <display_name>regular (BC Hydro)</display_name>
        </choice>
        <choice>
          <value>efficient (BC Hydro)</value>
          <display_name>efficient (BC Hydro)</display_name>
        </choice>
        <choice>
          <value>code (Quebec)</value>
          <display_name>code (Quebec)</display_name>
        </choice>
        <choice>
          <value>(non thermal bridging)</value>
          <display_name>(non thermal bridging)</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>write_tbd_json</name>
      <display_name>Write tbd.out.json</display_name>
      <description>Write tbd.out.json to customize for subsequent runs. Edit and place in model files directory as tbd.json</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>gen_kiva</name>
      <display_name>Generate Kiva inputs</display_name>
      <description>Generate OSM Kiva settings and objects if model surfaces have 'foundation' boundary conditions</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>gen_kiva_force</name>
      <display_name>Force-generate Kiva inputs</display_name>
      <description>Overwrites all 'ground' boundary conditions as 'foundation' before generating OSM Kiva inputs</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>false</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
